<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andy.money">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name="com.andy.contant.MyApplication"
        android:theme="@style/AppTheme">
        <activity android:name="com.andy.activity.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.andy.activity.AccountActivity" />
        <activity android:name="com.andy.activity.DetailActivity" />
        <activity android:name="com.andy.activity.MainActivity" />
        <activity android:name="com.andy.activity.LoginActivity" />
        <activity android:name="com.andy.activity.RegisterActivity"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>
